<div class="container">
<h2>Welcome, <%=@user.username%></h2>
<h1>Teams</h1>
 <ul id="teams">
    <% @teams.each.with_index(1) do |team, index| %>
        <li><a href="/teams/<%= team.id %>"><%= index %>: <%= team.team_name %></a></li>
    <% end %>
<br><br>


<a href="/create_team"><input type="submit" id="submit" value="Create Team"></a>
<br><br>
<a href="/players_list"><input type="submit" id="submit" value="Choose Player"></a>
<br></br>
<a href="/logout"><input type="submit" id="submit" value="Logout"></a>
</div>
