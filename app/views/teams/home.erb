<div class="container">
<h3>Welcome, <%=@user.username%></h3>
<h4>Here are your active fantasy baseball teams</h4>
  <id="teams">
    <% @teams.each.with_index(1) do |team, index| %>
        <li><a href="/teams/<%= team.id %>"><%= index %>: <%= team.name %></a></li>
    <% end %>
<br><br>


<a href="/teams/new"><input type="submit" id="submit" value="Create Team"></a>
<br><br>
<a href="/stadiums"><input type="submit" id="submit" value="Stadium List"></a>
<br></br>
<a href="/logout"><input type="submit" id="submit" value="Logout"></a>
</div>
